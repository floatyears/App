<h2>SingleAtkRecoverHP</h2>
<table class="table table-bordered table-hover">
	<thead>
		<tr>
      <th>baseInfo</th>
      <th>type</th>
      <th>value</th>
      <th>unitType</th>
      <th>attackType</th>
      <th>操作</th>
    </tr>
  </thead>
	<tbody>
    <% if  @all_skills[:SingleAtkRecoverHP]%>
    <% @all_skills[:SingleAtkRecoverHP].each_with_index do |skill,index|%>
    <tr>
      <td>
        {
          id:<%= skill[:baseInfo][:id]%>,
          name:<%= skill[:baseInfo][:name].force_encoding("UTF-8")%>,
          description:<%= skill[:baseInfo][:description].force_encoding("UTF-8")%>,
          skillCooling:<%= skill[:baseInfo][:skillCooling]%>
        }
      </td>
      <td><%=skill[:type] %></td>
      <td><%=skill[:value] %></td>
      <td><%=skill[:unitType] %></td>
      <td><%=skill[:attackType] %></td>
      <td><%=link_to "修改",edit_skill_path(skillType: "SingleAtkRecoverHP",id: index,method: "PATCH") %></td>
    </tr>
    <% end %>
    <% end %>
    
  </tbody>
</table>